	 X"$ac_prefix_program" : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X"$ac_prefix_program" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
    prefix=`$as_dirname -- "$prefix" ||
$as_expr X"$prefix" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$prefix" : 'X\(//\)[^/]' \| \
	 X"$prefix" : 'X\(//\)$' \| \
