<IfModule !knoweb_module>
  LoadModule knoweb_module      @apache_modules_dir@/mod_knoweb.so
</IfModule>
<IfModule mod_knoweb.c>
        AddHandler knoweb .knocgi .knoxml
	KnoExecutable @prefix@/bin/knoweb
	KnoUser @webuser@
	KnoGroup @admin_group@
	KnoPrefix @servlet_rundir@
	KnoLogPrefix @servlet_logdir@
</IfModule>
