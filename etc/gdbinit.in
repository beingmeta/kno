define slotmap
  printf "keyvals=%d\n",(&(((struct KNO_SLOTMAP *)$arg0)->sm_keyvals))
  printf "size=%d\n",((((struct KNO_SLOTMAP *)$arg0)->n_slots)&0x3FFFFFFF)
