#!@SCRIPT_SHELL@
prefix=@prefix@
exec_prefix=@exec_prefix@
option=$1;
if test -z ${option}; then option="usage"; fi
case ${option} in
    help|usage)
	echo version "\t\t\t(FramerD version)";
	echo major "\t\t\t\t(FramerD major version number)";
	echo minor "\t\t\t\t(FramerD minor version number)";
	echo release "\t\t\t(FramerD release version number)";
	echo prefix "\t\t\t\t(Installation prefix for FramerD)";
	echo execprefix "\t\t\t(Binary (executable) Installation prefix for FramerD)";
	echo config "\t\t\t\t(Default config directory)";
	echo localconfig "\t\t\t(Local config directory)";
	echo sharedconfig "\t\t\t(Shared config directory)";
	echo cflags "\t\t\t\t(Compiler flags for building executables)";
	echo ldflags "\t\t\t(Linker flags for building libraries)";
	echo include "\t\t\t(Include directory for FramerD)";
	echo libdir "\t\t\t\t(Directory where FramerD libraries are installed)";
	echo deplibs "\t\t\t\t(Libraries which FramerD is linked against)";
	echo libs "\t\t\t\t(Libraries refs for the FramerD core)";
	echo bin "\t\t\t\t(Where FramerD executables are installed)";
	echo libsuffix "\t\t\t(The suffix for shared objects)";
	echo loadpath "\t\t\t(The path used to search for source modules)";
	echo modulepath "\t\t\t(The path used to search for source modules)";
	echo safeloadpath "\t\t\t(The path used to search for sandboxed source modules)";
	echo safemodulepath "\t\t\t(The path used to search for sandboxed source modules)";
	echo builtin_modules "\t\t(Where built-in FramerD source modules live)";
	echo builtin_safe_modules "\t\t(Where built-in sanboxed source modules live)";
	echo installed_modules "\t\t(Where installed source modules live)";
	echo installed_safe_modules "\t\t(Where installed sandboxed modules live)";
	echo local_modules "\t\t\t(Where locally installed source modules live)";
	echo local_safe_modules "\t\t(Where locally installed sandboxed modules live)";
	echo cmodules "\t\t\t(Where compiled dynamic modules live)";
	echo webuser "\t\t\t(The name of the web content user account for this installation)";
	echo fdaemon "\t\t\t(The name of the fdaemon user account for this installation)";
	echo admin_group "\t\t\t(The name of the administrator account for this installation)";
	echo state "\t\t\t\t(Ephemeral state directory for FramerD)";
	echo rundir "\t\t\t\t(Where ephemeral state is stored for FramerD applications and services)";
	echo logdir "\t\t\t\t(Where persistent logs are stored for FramerD applications and services)";
	echo etc "\t\t\t\t(The directory this installation uses for 'etc')";
	echo servlets "\t\t\t(Where FramerD servlet configuration files are stored)";
	echo daemons "\t\t\t(Where FramerD daemon control files are stored)";
	echo share "\t\t\t\t(Where shared framerd configuration information is stored)";
	echo data "\t\t\t\t(Where FramerD persistent application data are stored)";;
    version)
	echo "@FDVERSION@";;
    major)
	echo "@FDMAJOR@";;
    minor)
	echo "@FDMINOR@";;
    release)
	echo "@FDRELEASE@";;
    prefix)
	echo "@prefix@";;
    execprefix|exec_prefix)
	echo "@exec_prefix@";;
    config)
	echo "@config_dir@";;
    localconfig|local_config)
	echo "@local_config@";;
    sharedconfig|shared_config)
	echo "@shared_config@";;
    cflags)
	echo "@CFLAGS@";;
    ldflags)
	echo "@LDFLAGS@ @PTHREAD_LIBS@ ${XLDFLAGS}";;
    include)
	echo "@prefix@/include/framerd";;
    lib)
	echo "@libdir@";;
    libs)
	echo "@LIBS@";;
    bin)
	echo "@exec_prefix@/bin";;
    libsuffix)
	echo "@shared_suffix@";;
    loadpath)
	echo "@default_module_path@";;
    safeloadpath)
	echo "@default_safe_module_path@";;
    modulepath)
	echo "@default_module_path@";;
    safemodulepath)
	echo "@default_safe_module_path@";;
    builtin_modules)
	echo "@builtin_module_dir@";;
    builtin_safe_modules)
	echo "@safe_module_dir@";;
    installed_modules)
	echo "@installed_module_dir@";;
    installed_safe_modules)
	echo "@installed_safe_module_dir@";;
    local_modules)
	echo "@local_module_dir@";;
    local_safe_modules)
	echo "@local_safe_module_dir@";;
    cmodules)
	echo "@libdir@/framerd";;
    webuser)
	echo "@webuser@";;
    fdaemon)
	echo "@fdaemon@";;
    admin|admingroup|admin_group)
	echo "@admin_group@";;
    state)
	echo "@localstatedir@";;
    rundir)
	echo "@framerd_rundir@";;
    logdir)
	echo "@framerd_logdir@";;
    etc)
	echo "@config_dir@";;
    servlets)
	echo "@config_dir@/servlets/";;
    daemons)
	echo "@config_dir@/daemons/";;
    share)
	echo "@share_dir@";;
    data)
	echo "@data_dir@";;
    *)
	echo "don't recognize config setting $1";;
esac
