DISTRIBUTION=${1-:stable}
URGENCY=medium
DATE=`date`
gitid=`git describe`
version=`git describe | sed -e "s/-g[0123456789ABCDEFabcdef]*$//"`
echo framerd \(${version}\) ${DISTRIBUTION}\; urgency=${URGENCY}
echo
echo "  * ${gitid} point release"
echo
echo " -- Repository Manager <repoman@beingmeta.com> ${DATE}"
echo
cat -
