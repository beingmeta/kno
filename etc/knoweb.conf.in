<IfModule !knoweb_module>
  LoadModule knoweb_module      @apache_modules_dir@/mod_knoweb.so
</IfModule>
<IfModule mod_knoweb.c>
        AddHandler knoweb .fdcgi .fdxml
	KnowebExecutable @prefix@/bin/knoweb
	KnowebUser @webuser@
	KnowebGroup @admin_group@
	KnowebPrefix @servlet_rundir@
	KnowebLogPrefix @servlet_logdir@
</IfModule>
