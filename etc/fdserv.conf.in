<IfModule !fdserv_module>
  LoadModule fdserv_module      @apache_modules_dir@/mod_fdserv.so
</IfModule>
<IfModule mod_fdserv.c>
        AddHandler fdservlet .fdcgi .fdxml
	FDServletExecutable @prefix@/bin/fdservlet
	FDServletUser @webuser@
	FDServletGroup @admin_group@
	FDServletPrefix @servlet_rundir@
	FDServletLogPrefix @servlet_logdir@
</IfModule>
