#!/bin/sh
fdstopall
if test -d @fdserv_log_dir@; then
  rm -f @fdserv_log_dir@/*.log @fdserv_log_dir@/*.log*
fi
if test -d @framerd_log_dir@; then
  rm -f @framerd_log_dir@/*.log @framerd_log_dir@/*.log*
fi
if test -d @rundir@/fdserv; then
  rm -f @rundir@/fdserv/*.sock @rundir@/fdserv/*.status \
        @rundir@/fdserv/*.run @rundir@/fdserv/*.sock \
        @rundir@/fdserv/*.pid;
  for tmpdir in @rundir@/fdserv/*.tmp;
  do if test -h ${tmpdir}; then
	  link_target=`readlink ${tmpdir}`; \
	      if test -d ${link_target}; then rm -rf ${link_target}; fi
	  rm -f ${tmpdir}; fi
  done
fi
if test -d @rundir@/framerd; then
  rm -f @rundir@/framerd/*.sock @rundir@/framerd/*.status \
        @rundir@/framerd/*.run @rundir@/framerd/*.sock \
        @rundir@/framerd/*.pid;
  for tmpdir in @rundir@/framerd/*.tmp;
  do if test -h ${tmpdir}; then
	  link_target=`readlink ${tmpdir}`; \
	      if test -d ${link_target}; then rm -rf ${link_target}; fi
	  rm -f ${tmpdir}; fi
  done
fi
