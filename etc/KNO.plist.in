<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>KNO</string>
    <key>Description</key>
    <string>KNO Server Manager</string>
    <key>ProgramArguments</key>
    <array>
      <string>@prefix@/bin/knoctl</string>
      <string>start</string>
    </array>
    <key>StandardOutPath</key>
    <string>@kno_logdir@/launchd.log</string>
    <key>StandardErrorPath</key>
    <string>@kno_logdir@/launchd_error.log</string>
    <key>ProcessType</key>
    <string>Background</string>
    <key>AbandonProcessGroup</key>
    <true/>
  </dict>
</plist>
