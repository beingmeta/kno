# Targets

default: all
all: testbin exe cmodules @TAGS@

# Handy targets

debug:
	make XCFLAGS="-O0" default
fresh:
	make clean; make default
freshdebug:
	make clean; make XCFLAGS="-O0" default
dbg:
	make clean; make XCFLAGS="-O0" default

nitpick:
	make XCFLAGS="-Wall" all

# configure/autoconf stuff

makefile: makefile.in config.status
	./config.status
./config.status: configure
	./config.status --recheck
configure: configure.ac
	autoconf

tests/dtype/makefile: tests/dtype/makefile.in ./configure
	configure
tests/db/makefile: tests/db/makefile.in ./configure
	configure
tests/scheme/makefile: tests/scheme/makefile.in ./configure
	configure
tests/dtype/.gdbinit: tests/dtype/.gdbinit.in ./configure
	configure
tests/db/.gdbinit: tests/db/.gdbinit.in ./configure
	configure
tests/scheme/.gdbinit: tests/scheme/.gdbinit.in ./configure
	configure
exe/.gdbinit: exe/.gdbinit.in ./configure
	configure
etc/fdgetconfig: etc/fdgetconfig ./configure
	configure
etc/fdsetconfig: etc/fdsetconfig ./configure
	configure
etc/framerd-rc.d: etc/framerd-rc.d ./configure
	configure
etc/OSXStartupItem/FramerD: etc/OSXStartupItem/FramerD.in ./configure
	configure
etc/logrotate: etc/logrotate.in ./configure
	configure
etc/fdserv.conf: etc/fdserv.conf.in ./configure
	configure
etc/fdserv.load: etc/fdserv.load.in ./configure
	configure

# Fileinfo gets version-related information about a file to pass in
# with -D

./fileinfo: etc/fileinfo.c
	$(CC) -o ./fileinfo etc/fileinfo.c

# Makes everything and then sudoes the install, keeping build files
# non-root

suinstall:
	make all
	sudo make install
dbginstall:
	make XCFLAGS="-O0" all
	sudo make install

# Make rules

%.o: %.c ./fileinfo
	@$(CC) $(CFLAGS) @definefileinfo@ -o $@ -c $<
	@$(MSG) CC $@ $<

exe/scripts/%: src/scripts/%.scm
	@$(ECHO) "#!"@prefix@/bin/fdexec | cat - $< > $@
	@chmod a+x $@
	@$(MSG) MAKESCRIPT $@ $<

exe/%@suffix@: src/exe/%.c
	@$(CC) $(CFLAGS) $(LDFLAGS) $(EXELDFLAGS)                   \
		-o $@ $<                                           \
		-lfdbserv@suffix@                 	 	   \
		-lfdweb@suffix@ -ltexttools@suffix@                \
		@xdynamic_scheme_libs@                             \
		-lfdscheme@suffix@                                 \
		-lfddbfile@suffix@ -lfddb@suffix@ -lfdtype@suffix@ \
	        -lu8stdio $(LIBS)
	@$(MSG) MKEXE $@ $<

# Custom rules for making include files with CSS headers
src/fdweb/xmloutput.o: src/fdweb/backtrace_css.h
src/fdweb/backtrace_css.h: src/fdweb/backtrace.css etc/text2include
	./etc/text2include FD_BACKTRACE_CSS $< $@

# We can selectively link some executables statically to avoid 
# library dependencies with programs which will typicall run as daemons
static/%@suffix@: src/exe/%.c $(STATIC_LIBS)
	@$(CC) $(EXECFLAGS) -o $@ $^ -lu8stdio -lu8syslog $(LIBS)
	@$(MSG) MKEXE_STATIC $@

exe/fdexec@suffix@ exe/fdbatch@suffix@ exe/fdconsole@suffix@: \
	$(STATIC_LIBS) $(SHARED_LIBS)
exe/fdcgiexec@suffix@ exe/fdbserver@suffix@ exe/fdserv@suffix@: \
	$(STATIC_LIBS) $(SHARED_LIBS)
exe/fdcgiexec@suffix@ exe/fdserv@suffix@: src/exe/webcommon.h

lib/%@suffix@.a:
	@$(MKSTATIC) $@ $^
	@$(MSG) MKSTATIC $@
lib/%@suffix@.so.@VERSION@:
	@$(MKSO) -Wl,-soname=$(@F) -o $@ $^ $(LIBS)
	@$(MSG) MKSO $@
lib/%@suffix@.so: lib/%.so.@VERSION@
	@ln -sf $(<F) $@
lib/%@suffix@.@VERSION@.dylib:
	@$(MACLIBTOOL) -install_name $(@F) -o $@ $^
	@$(MSG) MACLIBTOOL $@
lib/%@suffix@.dylib: lib/%@suffix@.@VERSION@.dylib
	ln -sf $(<F) $@

$(LIBINSTALLDIR)/%.a: lib/%.a
	@$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)
	@$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)
	@$(MSG) INSTALL $@

$(LIBINSTALLDIR)/framerd/%@suffix@.so.@VERSION@: \
		lib/framerd/%@suffix@.so.@VERSION@
	@$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)/framerd
	@$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)/framerd
	@$(MSG) INSTALL $@
$(LIBINSTALLDIR)/framerd/%@suffix@.so: \
		$(LIBINSTALLDIR)/framerd/%@suffix@.so.@VERSION@
	@ln -sf $(shell basename $<) $@

$(LIBINSTALLDIR)/%@suffix@.so.@VERSION@: lib/%@suffix@.so.@VERSION@
	@$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)
	@$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)
	@$(MSG) INSTALL $@
$(LIBINSTALLDIR)/%@suffix@.so: \
		$(LIBINSTALLDIR)/%@suffix@.so.@VERSION@
	@ln -sf $(shell basename $<) $@

$(LIBINSTALLDIR)/framerd/%@suffix@.@VERSION@.dylib: \
    lib/framerd/%@suffix@.@VERSION@.dylib $(LIBINSTALLDIR)/framerd
	@$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)/framerd
	@$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)/framerd
	@$(MSG) INSTALL $@
$(LIBINSTALLDIR)/framerd/%@suffix@.dylib: \
		$(LIBINSTALLDIR)/framerd/%@suffix@.@VERSION@.dylib
	@ln -sf $(shell basename $<) $@

$(LIBINSTALLDIR)/%@suffix@.@VERSION@.dylib: \
    lib/%@suffix@.@VERSION@.dylib $(LIBINSTALLDIR)
	@$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)
	@$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)
	@$(MSG) INSTALL $@
$(LIBINSTALLDIR)/%@suffix@.dylib: \
		$(LIBINSTALLDIR)/%@suffix@.@VERSION@.dylib
	@ln -sf $(shell basename $<) $@

$(BININSTALLDIR)/%: exe/%

$(BININSTALLDIR)/%: src/scripts/%.scm
	etc/install-script $<

# Custom rules for building fdmanager which doesn't actually use the
# FramerD libraries
exe/fdmanager@suffix@: src/exe/fdmanager.c
	@$(CC) $(CFLAGS) $(LDFLAGS) $(EXELDFLAGS)                   \
		-o $@ $<                                           \
		$(LIBS) -lu8stdio
	@$(MSG) MKEXE $@

# This can be helpful for some debugging 
etc/usememory: etc/usememory.c

