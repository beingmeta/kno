# The mod_fdserv module

etc/mod_fdserv.so: etc/mod_fdserv.c
	$(ECHO) "#define _FILEINFO \""`./fileinfo etc/mod_fdserv.c .`"\"" \
		> etc/mod_fdserv_fileinfo.h
	cd etc; ${APXS} -DUSEDTBLOCK=@USEDTBLOCK@ \
			-c mod_fdserv.c

mod_fdserv: etc/mod_fdserv.so

# For OSX, try this
# apxs -Wc,'-arch x86_64' -Wl,'-arch x86_64'  -a -i -c etc/mod_fdserv.c
install-fdserv: etc/mod_fdserv.so $(DESTDIR)@apache_modinfo@ $(DESTDIR)@apache_modules_dir@
	cd etc; ${INSTALL} @install_file_opts@ fdserv.conf fdserv.load $(DESTDIR)@apache_modinfo@;
	$(INSTALL) -m 0775 @wwwinstall@ @admininstall@ -d $(DESTDIR)@rundir@/fdserv;
	$(INSTALL) -m 0775 @wwwinstall@ @admininstall@ -d $(DESTDIR)@logdir@/fdserv;
	cd etc; if test -f mod_fdserv.so; then ${APXS} -i mod_fdserv.so; else ${APXS} -i mod_fdserv.la; fi

$(DESTDIR)@apache_modules_dir@:
	$(INSTALL) -m 0775 @wwwinstall@ @admininstall@ -d  $(DESTDIR)@apache_modules_dir@
$(DESTDIR)@apache_modinfo@:
	$(INSTALL) -m 0775 @wwwinstall@ @admininstall@ -d $(DESTDIR)@apache_modinfo@

update-fdserv: etc/mod_fdserv.so $(DESTDIR)@apache_modinfo@ \
		$(DESTDIR)@apache_modules_dir@
	@$(MSG) "STOPPING and STARTING Apache to INSTALL NEW mod_fdserv"
	@$(MSG) "THIS REQUIRES sudo PRIVILEGES; you may be prompted for your password"
	sudo apachectl stop
	cd etc; sudo ${INSTALL} @install_file_opts@ fdserv.conf fdserv.load \
			$(DESTDIR)@apache_modinfo@;
	sudo $(INSTALL) -m 0775 @wwwinstall@ @admininstall@ \
			-d $(DESTDIR)@rundir@/fdserv;
	sudo $(INSTALL) -m 0775 @wwwinstall@ @admininstall@ \
			-d $(DESTDIR)@logdir@/fdserv;
	cd etc; if test -f mod_fdserv.so; \
		then sudo ${APXS} -i mod_fdserv.so; \
		else sudo ${APXS} -i mod_fdserv.la; fi
	sleep 5; sudo apachectl start

# Tracking revision information

src/exe/fdversion.h: $(FRAMERD_SOURCES) $(FRAMERD_HEADERS)
	@if test -f etc/fdversion.h; \
	then cp etc/fdversion.h src/exe/fdversion.h; \
	else $(ECHO) "#define FRAMERD_REV \""`git describe`"\"" \
		> src/exe/fdversion.h; fi
	@$(MSG) FRAMERD_REV `git describe`
$(EXECUTABLES): src/exe/fdversion.h

