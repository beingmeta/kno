APXS		= @APXS@
CLEAN		= @CLEAN@
INSTALL		= @INSTALL@
MKSTATIC	= @MKSTATIC@
MACOSX_DEPLOYMENT_TARGET=10.3
MKSO		= $(CC) $(CFLAGS) $(XCFLAGS) -shared $(LDFLAGS)
