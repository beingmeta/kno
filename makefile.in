# Setup targets

confdirs: $(DESTDIR)@data_dir@ $(DESTDIR)@config_dir@ \
	  $(DESTDIR)@local_config@ $(DESTDIR)@shared_config@ \
	  $(DESTDIR)@module_dir@ $(DESTDIR)@safe_module_dir@
	if test ! -f $(DESTDIR)@config_dir@/servers; then $(INSTALL) @install_file_opts@ /dev/null $(DESTDIR)@config_dir@/servers; fi

$(DESTDIR)@data_dir@ $(DESTDIR)@data_dir@/etc $(DESTDIR)@config_dir@ \
  $(DESTDIR)@local_config@ $(DESTDIR)@shared_config@ \
  $(DESTDIR)@module_dir@ $(DESTDIR)@standard_module_dir@ $(DESTDIR)@safe_module_dir@: 
	$(INSTALL) @install_dir_opts@ $@

link-modules: $(DESTDIR)@module_dir@ \
              $(DESTDIR)@standard_module_dir@ \
              $(DESTDIR)@data_dir@/etc
	ln -sf $(BUILD)/etc/*.el $(CONFDIR)/etc
	rm -rf  $(DESTDIR)@standard_module_dir@
	ln -sf $(BUILD)/src/modules $(DESTDIR)@standard_module_dir@
	ln -sf $(BUILD)/src/fdweb/xhtml $(DESTDIR)@module_dir@
copy-modules: $(DESTDIR)@module_dir@ $(DESTDIR)@standard_module_dir@ \
              $(DESTDIR)@data_dir@/etc
	cp -pf $(BUILD)/etc/*.el $(CONFDIR)/etc
	$(INSTALL) @install_dir_opts@ \
           $(DESTDIR)@standard_module_dir@ \
	   $(DESTDIR)@module_dir@ \
	   $(DESTDIR)@safe_module_dir@
	cp -prf $(BUILD)/src/modules/* $(DESTDIR)@standard_module_dir@
	cp -prf $(BUILD)/src/fdweb/xhtml $(DESTDIR)@standard_module_dir@

$(DESTDIR)/etc/init.d/framerd: etc/framerd-rc.d $(DESTDIR)/etc/init.d
	$(INSTALL) -m 0555 @admininstall@ \
		   etc/framerd-rc.d $(DESTDIR)/etc/init.d/framerd
$(DESTDIR)/etc/init.d:
	$(INSTALL) -m 0775 @admininstall@ -d $(DESTDIR)/etc/init.d
$(DESTDIR)/var/run/framerd:
	$(INSTALL) -m 0775 @fdaemoninstall@ @admininstall@ \
		   -d $(DESTDIR)/var/run/framerd
$(DESTDIR)/var/log/framerd:
	$(INSTALL) -m 0775 @fdaemoninstall@ @admininstall@ \
		   -d $(DESTDIR)/var/log/framerd
setup-rc.d: $(DESTDIR)/etc/init.d/framerd \
            $(DESTDIR)/var/run/framerd \
            $(DESTDIR)/var/log/framerd
enable-rc.d: setup-rc.d
	update-rc.d framerd defaults

setup-osx: @data_dir@ $(DESTDIR)/var/run/framerd $(DESTDIR)/var/log/framerd
	etc/setup-osx "@data_dir@" \
		"${INSTALL} @install_dir_opts@" \
		"${INSTALL} @install_exe_opts@" \
		"${INSTALL} @install_exe_opts@" \
		/Library /etc

