# Targets

default: all
all: testbin exe cmodules @TAGS@
debug:
	make XCFLAGS="-O0" all
rebuild:
	make clean
	make default
fresh:
	make clean; make default
dbg:
	make clean; make XCFLAGS="-O0" default
# Makes everything and then sudoes install.  Keeps build files non-root
suinstall:
	make all
	sudo make install
dbginstall:
	make XCFLAGS="-O0" all
	sudo make install

libs: static-libs @SHARED_LIB_TARGET@
static-libs: $(STATIC_LIBS)
shared-libs: $(SHARED_LIBS)
testbin: dtypetestbin dbtestbin schemetestbin etc/usememory
dtypetestbin: libs  src/exe/revision.h
	cd tests/dtype; make
dbtestbin: libs  src/exe/revision.h
	cd tests/db; make
schemetestbin: libs  src/exe/revision.h
	cd tests/scheme; make
exe: $(EXECUTABLES)

etc/usememory: etc/usememory.c

cmodules: @CMODULES@

# Rules

exe/scripts/%: src/scripts/%.scm
	echo "#!"@prefix@/bin/fdexec | cat - $< > $@
	chmod a+x $@

exe/%@suffix@: src/exe/%.c
	$(CC) $(CFLAGS) $(LDFLAGS) $(EXELDFLAGS)                   \
		-o $@ $<                                           \
		$(LIBS) -lu8stdio                                  \
		-lfdtype@suffix@ -lfddb@suffix@ -lfddbfile@suffix@ \
		-lfdscheme@suffix@ -lfdschemeio@suffix@            \
		-lfdweb@suffix@ -ltexttools@suffix@                \
		-ltagger@suffix@ -lfdbserv@suffix@                 \
		@xdynamic_scheme_libs@

# We can selectively link some executables statically to avoid 
# library dependencies with programs which will typicall run as daemons
static/%@suffix@: src/exe/%.c $(STATIC_LIBS)
	$(CC) $(EXECFLAGS) -o $@ $^ $(LIBS) -lu8stdio -lu8syslog

exe/fdexec@suffix@ exe/fdbatch@suffix@ exe/fdconsole@suffix@: \
	$(STATIC_LIBS) $(SHARED_LIBS)
exe/fdcgiexec@suffix@ exe/fdbserver@suffix@ exe/fdserv@suffix@: \
	$(STATIC_LIBS) $(SHARED_LIBS)

%.o: %.c
	$(CC) $(CFLAGS) -o $@ -c $<

lib/%@suffix@.a:
	$(MKSTATIC) $@ $^
lib/%@suffix@.so.@VERSION@:
	$(MKSO) -Wl,-soname=$(@F) -o $@ $^
lib/%@suffix@.so: lib/%.so.@VERSION@
	ln -sf $(<F) $@
lib/%@suffix@.@VERSION@.dylib:
	$(MACLIBTOOL) -install_name $(@F) -o $@ $^
lib/%@suffix@.dylib: lib/%@suffix@.@VERSION@.dylib
	ln -sf $(<F) $@

$(LIBINSTALLDIR)/%.a: lib/%.a
	$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)
	$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)

$(LIBINSTALLDIR)/framerd/%@suffix@.so.@VERSION@: \
		lib/framerd/%@suffix@.so.@VERSION@
	$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)/framerd
	$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)/framerd
$(LIBINSTALLDIR)/framerd/%@suffix@.so: \
		$(LIBINSTALLDIR)/framerd/%@suffix@.so.@VERSION@
	ln -sf $(shell basename $<) $@

$(LIBINSTALLDIR)/%@suffix@.so.@VERSION@: lib/%@suffix@.so.@VERSION@
	$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)
	$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)
$(LIBINSTALLDIR)/%@suffix@.so: \
		$(LIBINSTALLDIR)/%@suffix@.so.@VERSION@
	ln -sf $(shell basename $<) $@

$(LIBINSTALLDIR)/framerd/%@suffix@.@VERSION@.dylib: \
    lib/framerd/%@suffix@.@VERSION@.dylib $(LIBINSTALLDIR)/framerd
	$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)/framerd
	$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)/framerd
$(LIBINSTALLDIR)/framerd/%@suffix@.dylib: \
		$(LIBINSTALLDIR)/framerd/%@suffix@.@VERSION@.dylib
	ln -sf $(shell basename $<) $@

$(LIBINSTALLDIR)/%@suffix@.@VERSION@.dylib: \
    lib/%@suffix@.@VERSION@.dylib $(LIBINSTALLDIR)
	$(INSTALL) @install_dir_opts@ $(LIBINSTALLDIR)
	$(INSTALL) @install_file_opts@ $< $(LIBINSTALLDIR)
$(LIBINSTALLDIR)/%@suffix@.dylib: \
		$(LIBINSTALLDIR)/%@suffix@.@VERSION@.dylib
	ln -sf $(shell basename $<) $@

$(BININSTALLDIR)/%: exe/%

$(BININSTALLDIR)/%: src/scripts/%.scm
	etc/install-script $<

# Custom rules for building fdmanager which doesn't actually use the
# FramerD libraries
exe/fdmanager@suffix@: src/exe/fdmanager.c
	$(CC) $(CFLAGS) $(LDFLAGS) $(EXELDFLAGS)                   \
		-o $@ $<                                           \
		$(LIBS) -lu8stdio

# Uninstall

LIBBASE=libfdtype libfddb libfddbfile libfdbserv \
	libfdscheme libfdschemeio libfdweb libtexttools libtagger

uninstall-core:
	./etc/uninstall $(DESTDIR)$(BININSTALLDIR) "" $(EXECUTABLES) fdcgiexec
	./etc/uninstall $(DESTDIR)$(BININSTALLDIR) "" $(SHELL_SCRIPTS)
	./etc/uninstall $(DESTDIR)$(BININSTALLDIR) "" $(SCHEME_SCRIPTS)
	./etc/uninstall $(DESTDIR)$(LIBINSTALLDIR) "@suffix@.a" $(LIBBASE)
	./etc/uninstall $(DESTDIR)$(LIBINSTALLDIR) "@suffix@.@VERSION@.dylib" $(LIBBASE)
	./etc/uninstall $(DESTDIR)$(LIBINSTALLDIR) "@suffix@.dylib" $(LIBBASE)
	./etc/uninstall $(DESTDIR)$(LIBINSTALLDIR) "@suffix@.@VERSION@.so" $(LIBBASE)
	./etc/uninstall $(DESTDIR)$(LIBINSTALLDIR) "@suffix@.so" $(LIBBASE)
	rm -rf $(LIBINSTALLDIR)/framerd
uninstall-all: uninstall-core
	rm -rf $(DESTDIR)@data_dir@ $(DESTDIR)@config_dir@ \
		$(DESTDIR)@local_config@ $(DESTDIR)@shared_config@ \
		$(DESTDIR)@module_dir@ $(DESTDIR)@safe_module_dir@
uninstall: uninstall-core
	@if (test -d $(DESTDIR)@data_dir@); then \
	  echo $(DESTDIR)@data_dir@ "still exists"; fi
	@if (test -d $(DESTDIR)@config_dir@); then \
	  echo $(DESTDIR)@config_dir@ "still exists"; fi
	@if (test -d $(DESTDIR)@local_config@); then \
	  echo $(DESTDIR)@local_config@ "still exists"; fi
	@if (test -d $(DESTDIR)@shared_config@); then \
	  echo $(DESTDIR)@shared_config@ "still exists"; fi
	@if (test -d $(DESTDIR)@module_dir@); then \
	  echo $(DESTDIR)@module_dir@ "still exists"; fi
	@if (test -d $(DESTDIR)@safe_module_dir@); then \
	  echo $(DESTDIR)@safe_module_dir@ "still exists"; fi

# Making dependency information

depend:
	makedepend -- $(CFLAGS) -- $(FDB_SOURCES)
depend.in:
	makedepend -fmakefile.in -Y -- $(CFLAGS) -- $(FDB_SOURCES)


# Utility targets

TAGS: tests/dtype/*.c tests/db/*.c tests/scheme/*.c src/exe/*.c       \
		src/dtype/*.c src/db/*.c src/scheme/*.c src/fdweb/*.c \
		src/texttools/*.c src/tagger/*.c src/cmodules/*.c     \
		include/fdb/*.h 
	etags tests/dtype/*.c tests/db/*.c tests/scheme/*.c src/exe/*.c \
	      src/dtype/*.c src/db/*.c src/scheme/*.c src/fdweb/*.c     \
	      src/texttools/*.c src/tagger/*.c src/cmodules/*.c         \
	      include/fdb/*.h

profile-clean:
	find . -name "*.gcda" | xargs rm -f
	find . -name "*.gcno" | xargs rm -f
	find . -name "*.gcov" | xargs rm -f
	find . -name "*.bb" | xargs rm -f
	find . -name "*.bbg" | xargs rm -f
	find . -name "*.da" | xargs rm -f
	find . -name "gmon.out" | xargs rm -f
tidy: profile-clean
	rm -f src/dtype/*~ src/db/*~ src/scheme/*~ src/fdweb/*~
	rm -f include/fdb/*~
	rm -f tests/dtype/*~ tests/db/*.o tests/scheme/*.o
	rm -f config.log
clean: tidy
	rm -f src/dtype/*.o src/db/*.o src/scheme/*.o src/fdweb/*.o 
	rm -f src/cmodules/*.o src/texttools/*.o src/tagger/*.o exe/*.o
	rm -rf etc/*.o etc/.libs
	rm -f lib/*.a lib/*@suffix@.so lib/*.dylib
	rm -f lib/*@suffix@.so.* lib/framerd/*
	rm -f exe/fdexec@suffix@ exe/fdbatch@suffix@
	rm -f exe/fdserv@suffix@ exe/fdbserver@suffix@
	rm -f exe/fdconsole@suffix@ exe/fdcgiexec@suffix@
	rm -f exe/fdmanager@suffix@
	rm -f src/exe/revision.h
	rm -f TAGS
	cd tests/dtype; make clean
	cd tests/db; make clean
	cd tests/scheme; make clean
	find . -name "*.dSYM" | xargs rm -rf
distclean: clean
	rm -f makefile config.log config.status config.cache
	rm -f tests/dtype/makefile tests/db/makefile tests/scheme/makefile
	rm -f etc/fdgetconfig etc/fdsetconfig etc/framerd-rc.d
	rm -f include/fdb/config.h

