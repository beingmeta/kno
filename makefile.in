# Setup targets

confdirs: @data_dir@ @config_dir@ @local_config@ @shared_config@ @module_dir@ @safe_module_dir@

@data_dir@ @config_dir@ @local_config@ @shared_config@ @module_dir@ @standard_module_dir@ @safe_module_dir@: 
	$(INSTALL) @install_dir_opts@ $@
	$(INSTALL) @install_file_opts@ /dev/null @config_dir@/servers
link-modules: @module_dir@ @standard_module_dir@ 
	ln -sf $(BUILD)/etc/*.el $(CONFDIR)/etc
	rm -rf  @standard_module_dir@
	ln -sf $(BUILD)/src/modules @standard_module_dir@
	ln -sf $(BUILD)/src/fdweb/xhtml @module_dir@
copy-modules: $(CONFDIR)/scm
	cp -pf $(BUILD)/etc/*.el $(CONFDIR)/etc
	$(INSTALL) @install_dir_opts@ @standard_module_dir@ @module_dir@ @safe_module_dir@
	cp -pf $(BUILD)/src/modules/*.scm @standard_module_dir@
	cp -pf $(BUILD)/src/modules/*.table @standard_module_dir@
	cp -rf $(BUILD)/src/fdweb/xhtml @module_dir@

$(DESTDIR)/etc/init.d/framerd:
	$(INSTALL) --mode=0555 etc/framerd-rc.d $(DESTDIR)/etc/init.d/framerd
$(DESTDIR)/var/run/framerd:
	$(INSTALL) --owner=fdaemon -d $(DESTDIR)/var/run/framerd
$(DESTDIR)/var/log/framerd:
	$(INSTALL) --owner=fdaemon -d $(DESTDIR)/var/log/framerd
setup-rc.d: $(DESTDIR)/etc/init.d/framerd \
            $(DESTDIR)/var/run/framerd \
            $(DESTDIR)/var/log/framerd
enable-rc.d: setup-rc.d
	update-rc.d framerd defaults

setup-osx: @data_dir@ $(DESTDIR)/var/run/framerd $(DESTDIR)/var/log/framerd
	etc/setup-osx "@data_dir@" \
		"${INSTALL} @install_dir_opts@" \
		"${INSTALL} @install_exe_opts@" \
		"${INSTALL} @install_exe_opts@" \
		/Library /etc

# The mod_fdserv module

etc/mod_fdserv.so: etc/mod_fdserv.c
	cd etc; ${APXS} -c mod_fdserv.c

mod_fdserv: etc/mod_fdserv.so

install-fdserv: etc/mod_fdserv.so
	if test -d @apache_conf_dir@/mods-available; \
	   then	cd etc; ${APXS} -i -c mod_fdserv.c;  \
		${INSTALL} @install_file_opts@ \
		    fdserv.conf fdserv.load \
		    @apache_conf_dir@/mods-available; \
	elif test -d @apache_conf_dir@.d; \
	   then cd etc; ${APXS} -i -a -c mod_fdserv.c; \
		${INSTALL} @install_file_opts@ \
		    fdserv.conf \
		    @apache_conf_dir@.d; \
	fi
	$(INSTALL) -o $(WWWUSER) -d $(DESTDIR)/var/run/fdserv
	$(INSTALL) -o $(WWWUSER) -d $(DESTDIR)/var/log/fdserv

# Tracking revision information

src/exe/revision.h: $(FDB_SOURCES) $(FDB_HEADERS)
	echo "#define SVN_REVISION \""`svnversion .`"\"" > src/exe/revision.h
$(EXECUTABLES): src/exe/revision.h

