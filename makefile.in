	$(INSTALL) @install_dir_opts@ $(CONFDIRS)
	cp -pf $(BUILD)/etc/*.el $(DESTDIR)@data_dir@
	$(INSTALL) @install_dir_opts@ \
           $(DESTDIR)@standard_module_dir@ \
	   $(DESTDIR)@module_dir@ \
	   $(DESTDIR)@safe_module_dir@
	cp -pRf $(BUILD)/src/modules/* $(DESTDIR)@standard_module_dir@
	cp -pRf $(BUILD)/src/modules/sandbox/* $(DESTDIR)@safe_module_dir@
	cp -pRf $(BUILD)/src/fdweb/xhtml $(DESTDIR)@safe_module_dir@
	cp -pRf $(BUILD)/src/beingmeta/* $(DESTDIR)@module_dir@

