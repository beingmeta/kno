MACLIBTOOL	= MACOSX_DEPLOYMENT_TARGET=10.3 \
		  $(CC) -dynamiclib -single_module \
			-undefined dynamic_lookup \
			$(LDFLAGS) $(LIBS)
BUILD		= `pwd`
