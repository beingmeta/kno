command source @abs_top_srcdir@/lldbinit
#directory @abs_top_srcdir@:@abs_top_srcdir@/../libu8:@abs_top_srcdir@/../lingo:@extra_sources@
#set env KNO_POOLS /data/bg/brico.pool
#set env KNO_INDICES /data/bg/brico.index
#set env KNO_POOLS bground@localhost
#set env KNO_INDICES bground@localhost
env DYLD_LIBRARY_PATH=@abs_top_srcdir@/lib
env LD_LIBRARY_PATH=@abs_top_srcdir@/lib
env KNO_INIT_DLOADPATH @abs_top_srcdir@/lib/kno/%.@shared_suffix@:@abs_top_srcdir@/lib/libkno%.@shared_suffix@:@abs_top_srcdir@/../lingo/%.@shared_suffix@:@prefix@/lib/kno/%.@shared_suffix@
env KNO_INIT_LOADPATH=@abs_top_srcdir@/src/extmods/:@abs_top_srcdir@/src/extmods/safe/:@default_module_path@
env KNO_INIT_SAFELOADPATH=@abs_top_srcdir@/src/extmods/safe/:@default_safe_module_path@
env KNO_UPDATEMODULES=5
env KNO_SKIP_DOTLOAD=yes
env KNO_TRACECONFIGLOAD=yes
env KNO_DLOAD:TRACE=yes
env KNO_SHOWTIME=0.0
env KNO_DISPLAYMAXELTS=36
env KNO_DISPLAYMAXCHARS=1024
env KNO_TRACEWEB=yes
env KNO_WEBDEBUG=yes
env KNO_WEBALLOWDEBUG=yes
env KNO_LOADMOD=kno/sessions
env KNO_LIBSCM_DIR=@abs_top_srcdir@/src/libscm/
